{% extends '::customer/backend.html.twig' %}
{% block stylesheets %}
   {{ parent() }}
<link rel="stylesheet" href="{{ asset('css/themes/proton/style.min.css')}}" />

{% endblock %}

{% block container -%}
    <h3>Nueva Sucursal</h3>
    
   
    <div class="well">
    <div id="myTabContent" class="tab-content">
    <div class="tab-pane active in" id="home">
    <form class="form-horizontal" id="tab" name="tab" method="post" novalidate="true" {{ form_enctype(form) }}   action="{{ path('sucursal_create') }}" >
     {{ form_errors(form) }}
    
    {% include 'BackendCustomerAdminBundle:Sucursal:_sucursal.html.twig' %} 

    
    {{ form_rest(form) }}

    <div class="form-group">
    <!-- Button -->
    <div class="text-center">
    <button type="submit" class="btn btn-primary"><i class="fa fa-plus"></i>Crear</button>
    <button type="button" class="btn btn-inverse" data-url="{{ path('sucursal') }}"><i class="fa fa-chevron-left"></i> Volver</button>
    
    </div>
    
    </div>
    </form>
    </div>
    
     </div>
      </div>
    

       
{% endblock %}
{% block javascripts %}
  {{ parent() }}
  <script type="text/javascript">
   	Shadowbox.init();
  </script>
    <script type="text/javascript" src="{{ asset('js/jstree.min.js') }}"></script>
{% javascripts '@BackendCustomerAdminBundle/Resources/public/js/validate_sucursal.js' filter='?uglifyjs2' %}
    <script type="text/javascript" src="{{ asset_url }}"></script>
{% endjavascripts %}
{% endblock %}
