<!doctype html>
<html class="no-js" lang="">
    <head>
        <meta charset="utf-8">
        <meta http-equiv="x-ua-compatible" content="ie=edge">
        <title>{% block title %}Zona Delivery{% endblock %}</title>
        <meta name="description" content="">
        <meta name="author" content="">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <base href="{{ app.request.getBaseURL() }}" />
        {% block stylesheets %}
          <link href="{{ asset("css/front/bootstrap.min.css")}}" rel="stylesheet">
          <link href="{{ asset("css/front/font-awesome.min.css")}}" rel="stylesheet">
          <link href="{{ asset("css/front/prettyPhoto.css")}}" rel="stylesheet">
          <link href="{{ asset("css/front/price-range.css")}}" rel="stylesheet">
          <link href="{{ asset("css/front/animate.css")}}" rel="stylesheet">
      	  <link href="{{ asset("css/front/main.css")}}" rel="stylesheet">
		      <link href="{{ asset("css/front/freelancer.css")}}" rel="stylesheet">
      	  <link href="{{ asset("css/front/responsive.css")}}" rel="stylesheet">
          <link href="{{ asset("css/front/custom.css")}}" rel="stylesheet">
          <link rel="stylesheet" href="//code.jquery.com/ui/1.11.4/themes/smoothness/jquery-ui.css">
          <link rel="shortcut icon" href="{{ asset("images/ico/favicon.ico")}}">
          <link rel="apple-touch-icon-precomposed" sizes="144x144" href="{{ asset("images/ico/apple-touch-icon-144-precomposed.png")}}">
          <link rel="apple-touch-icon-precomposed" sizes="114x114" href="{{ asset("images/ico/apple-touch-icon-114-precomposed.png")}}">
          <link rel="apple-touch-icon-precomposed" sizes="72x72" href="{{ asset("images/ico/apple-touch-icon-72-precomposed.png")}}">
          <link rel="apple-touch-icon-precomposed" href="{{ asset("images/ico/apple-touch-icon-57-precomposed.png")}}">
          <link href="{{ asset("js/sweet-alert/sweetalert.css")}}" rel="stylesheet">
        {% endblock %}
        
    </head>
    <body>
    <!--[if lt IE 9]>
        <p class="browserupgrade">You are using an <strong>outdated</strong> browser. Please <a href="http://browsehappy.com/">upgrade your browser</a> to improve your experience.</p>
    <![endif]-->
    {% block body %}
        {% block menu %}
            {% include '::front/menu.html.twig' %}        
        {% endblock %}
        {% block header %}
            {% include '::front/header.html.twig' %}        
        {% endblock %}
        {% block container %}
        {% endblock %}
        {% block footer %}
            {% include '::front/footer.html.twig' %}        
        {% endblock %}
    {% endblock %}
    {% block javascripts %}
        <script type="text/javascript" src="//maps.google.com/maps/api/js?sensor=true"></script>
        <script src="{{ asset("js/moment.min.js") }}"></script>
        <script src="{{ asset("js/jquery.js") }}"></script>
      	<script src="{{ asset("js/bootstrap.min.js") }}"></script>
      	<script src="{{ asset("js/front/jquery.scrollUp.min.js") }}"></script>
      	<script src="{{ asset("js/front/price-range.js") }}"></script>
        <script src="{{ asset("js/front/jquery.prettyPhoto.js") }}"></script>
        <script src="//code.jquery.com/ui/1.11.4/jquery-ui.js"></script>
        <script src="{{ asset("js/front/jquery.cookie.js") }}"></script>
        <script src="{{ asset('js/jquery.validate.min.js') }}"></script>
        <script src="{{ asset('js/front/simpleCart.js') }}"></script>
        <script src="{{ asset('js/sweet-alert/sweetalert.min.js') }}"></script>
        <script src="{{ asset('js/vendor/typeahead.bundle.js') }}"></script>

        {% javascripts '@FrontendHomeBundle/Resources/public/js/constants.js'  filter='?uglifyjs2' %}
          <script type="text/javascript" src="{{ asset_url }}"></script>
        {% endjavascripts %}
        
        {% javascripts '@FrontendHomeBundle/Resources/public/js/utils.js'  filter='?uglifyjs2' %}
          <script type="text/javascript" src="{{ asset_url }}"></script>
        {% endjavascripts %}
        
        {% javascripts '@FrontendHomeBundle/Resources/public/js/main.js'  filter='?uglifyjs2' %}
          <script type="text/javascript" src="{{ asset_url }}"></script>
        {% endjavascripts %}
        
        {% javascripts '@FrontendHomeBundle/Resources/public/js/carrito.js'  filter='?uglifyjs2' %}
          <script type="text/javascript" src="{{ asset_url }}"></script>
        {% endjavascripts %}
         
        {% javascripts '@FrontendHomeBundle/Resources/public/js/register_user.js'  filter='?uglifyjs2' %}
          <script type="text/javascript" src="{{ asset_url }}"></script>
        {% endjavascripts %}
        
        
    {% endblock %}
    </body>
</html>    
